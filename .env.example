# Azure Document Intelligence
AZURE_DOCUMENTINTELLIGENCE_ENDPOINT=https://<seu-resource-name>.cognitiveservices.azure.com/
AZURE_DOCUMENTINTELLIGENCE_KEY=<sua-api-key>

# Regras padrão
MIN_CONFIDENCE=0.6
REQUIRE_SUM_MATCH=false
SUM_TOLERANCE=0.01

# Lista de fornecedores permitidos (se vazia, não valida este critério)
VENDORS_ALLOWLIST=Fornecedor A,Fornecedor B
